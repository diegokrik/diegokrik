#include<stdio.h>

int main(void) {
	int di, hi, mi, si, df, hf, mf, sf, W, X, Y, Z;
	char a, b, c, d, e, f;
	scanf("%s %d", &a, &di);	
	scanf("%d %s %d %s %d", &hi, &b, &mi, &c, &si);
	scanf("%s %d", &d, &df);
	scanf("%d %s %d %s %d", &hf, &e, &mf, &f, &sf);
	W = df - di;
	if(hi == hf) 
	X = 0;
	else if(hi < hf)
	X = hf - hi;
	else
	X = 24 - hi + hf;
	if(mi == mf)
	Y = 0;
	else if(mi < mf)
	Y = mf - mi;
	else
	Y = 60 - mi + mf;
	if(si == sf)
	Z = 0;
	else if(si < sf)
	Z = sf - si;
	else
	Z = 60 - si + sf;
	printf("%d dia(s)\n", W);
	printf("%d hora(s)\n", X);
	printf("%d minuto(s)\n", Y);
	printf("%d segundo(s)\n", Z);
	return 0;
}
